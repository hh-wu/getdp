    NameOfFormulation @var{formulation-id} <@{@}> < [ @var{mesh-id} ] >; 
    PostQuantity @{
      @{ Name @var{post-quantity-id}; Type @var{post-quantity-type}; 
        Value @{ @var{post-value} @dots{} @} @} @dots{}
    @}
  @} @dots{}
@}
@end example

